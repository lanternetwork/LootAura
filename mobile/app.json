{
  "expo": {
    "name": "LootAura",
    "slug": "LootAura",
    "scheme": "lootaura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3A2268"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.lootaura.app",
      "versionCode": 90,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3A2268"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "lootaura.com",
              "pathPrefix": "/auth/callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "lootaura.com",
              "pathPrefix": "/auth/native-callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [],
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#ffffff",
        "hidden": false,
        "translucent": false
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lootaura.app",
      "associatedDomains": [
        "applinks:lootaura.com"
      ],
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "LootAura uses your location to show nearby yard sales."
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "kotlinVersion": "1.9.25"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "07dcac78-a086-4c00-9174-07984586ab86"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "lantern-networks"
  }
}
